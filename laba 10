#dz1
//весь предыдущий код +
void print1000_0() {

	int i, n;

	i = 1000;
	n = 9;

	while (i >= 0) {
		printf("%d ", i);
		i -= n;
		if (i < 0) {
			printf("%d ", 0);
		}
	}
}
case 20:
	print1000_0();
	break;
//////////////////////////////////
#dz2
//весь предыдущий код +
void Factorialprint() {

	int i, a, n;
	printf("Введите кол-во факториалов N = ");
	scanf_s("%d", &n);
	i = 1;
	a = 1;
	while (i <= n) {
		a = a * i;
		printf("%d ", a);
		i++;
	}
}
case 21:
	Factorialprint();
	break;
