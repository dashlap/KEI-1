#include <stdio.h>
#include <Windows.h>

void print10() {
	int = 1;
	do {
		printf("%d", a);
		a += 1;
	} while (a <= 10);
}

void print11() {
	int a = 10;

	do {
		printf("%d", a);
		a -= 1;
	} while (a >= 1);
}

void print12() {
	int a = 1;
	int b = 1;

	do {
		printf("%d", a);

		a += 2;
		b += 1;

	} while (b <= 5);
}

void print100_10_while() {
	int a = 100;
	while (a >= 10) {
		print("%d", a);
		a -= 10;
	}
}
void main() {
	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);

	int i;

	do {
		printf("\n");
		printf("\n");
		printf("Выберите нужную Вам операцию\n");
		printf("1: Вывести числа от 1 до 10\n");
		printf("2: Вывести числа от 10 до 1\n");
		printf("3: Вывести 5 первых нечетных чисел, начиная с 1\n");
		printf("\n");
		printf("0: Выйти из программы\n");
		scanf_s("%d", &i);

		switch (i) {
		case 1:
			print10();
			break;
		case 2:
			print11(;
			break;
		case 3:
			print12();
			break;
		case 11:
			print100_10_while();
			break;
		}

	} while (i != 0);
}
