{//куст1
    int x1 = 400, y1 = 150;
    int x2 = 500, y2 = 250;

    do {
        MoveToEx(hdc, x1, y1, NULL);
        LineTo(hdc, x2, y2);
        x1 += 10;
    } while (x1 <= 600);
}
{//куст2
    int x1 = 0, y1 = 250;
    int x2 = 100, y2 = 250;

    do {
        MoveToEx(hdc, x1, y1, NULL);
        LineTo(hdc, x2, y2);
        x1 += 10;
        y1 -= 20;
    } while (x1 <= 100);

    x1 = 100;
    y1 = 50;
    do {
        MoveToEx(hdc, x1, y1, NULL);
        LineTo(hdc, x2, y2);
        x1 += 10;
        y1 += 20;
    } while (x1 <= 200);
}

{//забор
    int x1 = 150, y1 = 0;
    int x2 = 150, y2 = 100;
    do {
        MoveToEx(hdc, x1, y1, NULL);
        LineTo(hdc, x2, y2);
        x1 += 25;
        x2 += 25;
    } while (x1 <= 450);
}
