MoveToEx(hdc, 75, 340, NULL);
LineTo(hdc, 75, 525);
MoveToEx(hdc, 75, 375, NULL);
LineTo(hdc, 55, 360);
MoveToEx(hdc, 75, 400, NULL);
LineTo(hdc, 100, 375);
