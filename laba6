#1

#include
void main() {
	int i;
	int n;

	printf("n = ");
	scanf_s("%d", &n);

	i = 1;
	do {
		printf("%d ", i);
		i = i + 1;
	} while (i <= n);
}
//////////////////////////////
#2
#include
void main() {
	int i;
	int stepen;

	stepen = 1;
	i = 0;
	do {
		printf("2^%d = %d\n", i, stepen);
		i = i + 1;
		stepen = stepen * 2;
	} while (i <= 5);
}
////////////////////////////////////
#dz1

#include <stdio.h>
#include <Windows.h>


void main() {

	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);

	int n;
	int i;

	printf("Введите число чисел, которых надо вывести = ");
	scanf_s("%d", &n);
	i = 2;
	do {
		printf("%d ", i);
		i = i + 2;

	} while (i <= n * 2);
}
