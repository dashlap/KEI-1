#dz1

#include <stdio.h>
#include <math.h>
#include <Windows.h>

void main()
{
	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);


	int temp;

	printf("Введите температуру в Египте: ");
	scanf_s("%d", &temp);

	if (temp < 10) {
		printf("Мороз");
	}
	if ( 10 <= temp && temp  < 15) {
		printf("Холодно");
	}
	if (15 <= temp && temp <= 30) {
		printf("Тепло");
	}
	if (temp > 30) {
		printf("Жарко");
	}
}
//////////////////////////////////////////////////////
#dz2

#include <stdio.h>
#include <math.h>
#include <Windows.h>

void main()
{
	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);


	int cena;

	printf("Введите стоимость однокомнатной квартиры: ");
	scanf_s("%d", &cena);

	if (cena < 1000000) {
		printf("Дешево");
	}
	if ( 1000000 <= cena && cena  < 2000000) {
		printf("Нормально");
	}
	if (2000000 <= cena && cena < 3500000) {
		printf("Дорого");
	}
	if (cena > 3500000) {
		printf("Ужасно дорого");
	}
}
/////////////////////////////////////////////////////
#dz3

#include <stdio.h>
#include <math.h>
#include <Windows.h>

void main()
{
	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);


	int v;

	printf("Введите скорость автомобиля: ");
	scanf_s("%d", &v);

	if ( v > 15 && v < 25) {
		printf("Медленно");
	}
	else if ( v <= 15) {
		printf("Очень медлено");
	}
	else if (v >= 25 && v < 60) {
		printf("Нормально");
	}
	else if (v >= 60 && v < 100) {
		printf("Быстро");
	}
	else if (v >= 100 && v < 160) {
		printf("Очень быстро");
	}	

}
